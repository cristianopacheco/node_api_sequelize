{
  "name": "node_api_sequelize",
  "version": "0.0.1",
  "description": "Boilerplate node API + sequelize",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "test": "NODE_ENV=test npm run test:unit && npm run test:acceptance",
    "test:acceptance":
      "NODE_ENV=test ./node_modules/.bin/mocha --opts test/acceptance/mocha.opts test/acceptance/**/*_spec.js",
    "test:unit":
      "NODE_ENV=test ./node_modules/.bin/mocha --opts test/unit/mocha.opts test/unit/**/*_spec.js",
    "lint": "./node_modules/.bin/eslint '**/*.js'",
    "lint:fix": "./node_modules/.bin/eslint '**/*.js' --fix"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cristianopacheco/node_api_sequelize.git"
  },
  "engines": {
    "node": "~8.4.0",
    "npm": "~5.4.2"
  },
  "keywords": [],
  "author": "Cristiano Pacheco <chris.spb25@gmail.com>",
  "license": "ISC",
  "dependencies": {
    "body-parser": "^1.18.2",
    "express": "^4.16.2"
  },
  "devDependencies": {
    "chai": "^4.1.2",
    "eslint": "^4.10.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-react": "^7.4.0",
    "mocha": "^4.0.1",
    "sinon": "^4.1.1",
    "supertest": "^3.0.0"
  }
}
